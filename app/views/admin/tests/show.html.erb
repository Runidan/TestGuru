<h1>Test: <%= @test.title%></h1>
<p>Level <%= @test.level%></p>

<%= link_to "New Question", new_test_question_url(@test) %> | 
<%= link_to "Edit", edit_admin_test_path(@test) %> | 
<%= link_to "Back", admin_tests_path %>

<% if @test.questions.size != 0%>
  <h3>Questions list</h3>
  <table>
    <tr>
      <th>Questions</th>
      <th colspan=3>Actions</th>
    </tr>
    
      <%= render partial: "questions/question", collection: @test.questions %>
    
  </table>
<% else %>
  <p>Question is not exist</p>
<% end %>
